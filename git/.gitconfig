[core]
	excludesfile = ~/.gitexcludes
	editor = /usr/bin/vim
	quotepath = false
[user]
	name = Kevin Baribeau
	email = kevin.baribeau@gmail.com
[alias]
	ci = commit
	lg = log --date=relative --stat --graph
	praise = blame
	st = status -s
	aa = add --all
	ca = commit --amend
	co = checkout
	cp = cherry-pick
	dc = diff --cached
	di = diff
	head = !git lg -1
	pom = push origin master
	pr = pull --rebase
	rc = rebase --continue
	ri = rebase --interactive
	rp = remote prune

	clown = clone
	stache = stash

	fixup = commit --fixup
[color]
	ui = auto
[branch]
	autosetuprebase = always
[github]
	user = kbaribeau
[rerere]
	enabled = 1
[rebase]
	autosquash = true
[git-pair]
	authors = Zee Spencer <zee@zeespencer.com>
	authors = Kevin Baribeau <kevin.baribeau@gmail.com>
	authors = Elisabeth Hendrickson <esh@qualitytree.com>
[push]
	default = current
[include]
  path = ~/.gitconfig.local
[credential]
	helper = osxkeychain
